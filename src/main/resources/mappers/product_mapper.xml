<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.woofnmeow.wnm_project_back.repository.ProductMapper">
    <insert id="addProduct" parameterType="com.woofnmeow.wnm_project_back.entity.Product">
        insert into
            product_tb
        values(0, #{productName}, #{productStock}, #{productPrice}, #{productDetailText}, #{productThumbnail}, #{productDetailImg}, #{categoryId}, #{productSizeXs}, #{productSizeS}, #{productSizeM}, #{productSizeL}, #{productSizeXL}, #{productSizeXXL})
    </insert>

</mapper>