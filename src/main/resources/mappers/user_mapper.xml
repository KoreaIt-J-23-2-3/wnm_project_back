<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.woofnmeow.wnm_project_back.repository.UserMapper">

    <insert id="saveUser" parameterType="com.woofnmeow.wnm_project_back.entity.User">
        insert into
            user_tb
        values(0, 'ROLE_USER', #{oauth2Id}, #{provider}, #{name}, #{nickname}, #{email}, #{phoneNumber}, #{defaultAddressNumber}, #{defaultAddressName}, #{defaultAddressDetailName}, default)
    </insert>

</mapper>